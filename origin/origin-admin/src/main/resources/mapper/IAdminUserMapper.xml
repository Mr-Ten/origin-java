<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.origin.admin.mapper.IAdminUserMapper">

    <select id="findUserName" resultType="com.origin.admin.entity.po.AdminUser">
        SELECT
            au.user_id userId, au.account, au.`password`, au.salt
        FROM admin_user au
        WHERE au.account = #{account}
    </select>
</mapper>
